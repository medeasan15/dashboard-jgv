    <style>
/* FEMINIM STYLE SPEEDOMETER */

* {
    user-select: none;
    user-drag: none;
    box-sizing: border-box;
}

:root {
    /* Feminim Color Palette */
    --fem-bg: #fff0f5; /* Lavender Blush */
    --fem-surface: #ffe6f0; /* Light Pink */
    --fem-panel: #ffd6e7; /* Pink Mist */
    --fem-text-primary: #800080; /* Purple */
    --fem-text-secondary: #c71585; /* Medium Violet Red */
    --fem-accent-pink: #ff69b4; /* Hot Pink */
    --fem-accent-lavender: #dda0dd; /* Plum */
    --fem-accent-rose: #ffb6c1; /* Light Pink */
    --fem-grid: #e6bcde; /* Soft Purple */
    --fem-heart-icon: #ff6b9d; /* Coral Pink */

    /* Typography */
    --font-fem-primary: 'Dancing Script', cursive;
    --font-fem-secondary: 'Quicksand', sans-serif;

    /* Dimensions */
    --dashboard-width: 450px;
    --dashboard-height: 400px;
}

body {
    background: transparent!important;
    font-family: var(--font-fem-secondary);
    color: var(--fem-text-primary);
    display: flex;
    justify-content: center;
    align-items: center;
    min-height: 100vh;
    margin: 0;
    padding: 1rem;
}

/* Dashboard Loading Indicator */
.dashboard-loading {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    z-index: 100;
    animation: fadeOutDashboardLoading 0.5s ease-out 2.5s forwards;
}

.dashboard-spinner {
    width: 40px;
    height: 40px;
    border: 2px solid var(--fem-grid);
    border-top: 2px solid var(--fem-accent-pink);
    border-radius: 50%;
    animation: spin 1s linear infinite;
    margin-bottom: 10px;
}

.dashboard-loading-text {
    font-family: var(--font-fem-primary);
    font-size: 1rem;
    color: var(--fem-accent-pink);
    letter-spacing: 1px;
    animation: loadingTextPulse 1.5s ease-in-out infinite;
}

@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

@keyframes loadingTextPulse {
    0%, 100% { opacity: 0.6; }
    50% { opacity: 1; }
}

@keyframes fadeOutDashboardLoading {
    0% { opacity: 1; visibility: visible; }
    100% { opacity: 0; visibility: hidden; }
}

/* Main Dashboard - Curved/Rounded Design */
.dashboard-container {
    position: fixed;
    bottom: 35px;
    right: 0px;
    width: var(--dashboard-width);
    height: var(--dashboard-height);
    background: transparent !important;
    border-top-left-radius: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
}

.dashboard-container::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: transparent;
    opacity: 0.95;
    z-index: -11;
}

/* Main Speedometer Container */
.speedometer-container {
    position: fixed;
    bottom: 60px;
    right: 100px;
    transform: translate(-50%, -50%);
    z-index: 21000;
}

/* Central Speedometer - Rounded Design */
.speedometer {
    position: relative;
    width: 280px;
    height: 160px;
    border-radius: 20px;
    background-color: var(--fem-panel);
    border: 2px solid var(--fem-grid);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    box-shadow: inset 0 0 15px rgba(221, 160, 221, 0.3);
    overflow: hidden;
}

/* Decorative elements */
.speedometer::before {
    content: "";
    position: absolute;
    top: -10px;
    left: -10px;
    right: -10px;
    bottom: -10px;
    background: radial-gradient(circle, rgba(255,182,193,0.1) 0%, rgba(255,182,193,0) 70%);
    z-index: -1;
}

/* Center digital display */
.speed-center {
    position: relative;
    z-index: 5;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
}

.speed-display {
    font-family: var(--font-fem-primary);
    font-size: clamp(6rem, 8vw, 1.8rem);
    font-weight: 700;
    line-height: 1;
    color: var(--fem-accent-pink);
    text-shadow: 0 0 8px rgba(255, 105, 180, 0.5);
    transition: color 0.3s;
}

.speed-unit {
    font-family: var(--font-fem-secondary);
    font-size: clamp(2rem, 2vw, 0.7rem);
    color: var(--fem-text-secondary);
    margin-top: 5px;
}

/* Horizontal Indicators Container - Rounded */
.horizontal-indicators-container {
    position: fixed;
    bottom: 10px;
    right: 10px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 370px;
    height: 40px;
    background-color: var(--fem-panel);
    border-radius: 20px;
    padding: 8px 15px;
    z-index: 25000;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
}

.horizontal-indicators-container .icon {
    position: relative;
    width: 30px;
    height: 30px;
    fill: var(--fem-text-secondary);
    stroke: var(--fem-text-secondary);
    border-radius: 50%;
    background: transparent;
    padding: 6px;
    border: 1px solid rgba(199, 21, 133, 0.3);
    flex-shrink: 0;
}

.horizontal-indicators-container .icon:hover {
    transform: scale(1.1);
}

.horizontal-indicators-container .icon.active {
    fill: var(--fem-accent-pink);
    stroke: var(--fem-accent-pink);
    border-color: var(--fem-accent-pink);
    box-shadow: 0 0 12px rgba(255, 105, 180, 0.7);
}

/* Turn Indicator Icons - positioned at the ends */
.horizontal-indicators-container #left-indicator-icon {
    order: 1;
}

.horizontal-indicators-container #right-indicator-icon {
    order: 5;
}

/* Middle icons */
.horizontal-indicators-container #engine-icon {
    order: 2;
}

.horizontal-indicators-container #headlights-icon {
    order: 3;
}

.horizontal-indicators-container #seatbelt-icon {
    order: 4;
}

/* Specific icon states */
.horizontal-indicators-container #headlights-icon.low-beam {
    fill: var(--fem-accent-lavender);
    stroke: var(--fem-accent-lavender);
    border-color: var(--fem-accent-lavender);
    box-shadow: 0 0 12px rgba(221, 160, 221, 0.7);
}

.horizontal-indicators-container #headlights-icon.high-beam {
    fill: #ba55d3; /* Medium Orchid */
    stroke: #ba55d3;
    border-color: #ba55d3;
    box-shadow: 0 0 12px rgba(186, 85, 211, 0.7);
}

.horizontal-indicators-container #seatbelt-icon.active {
    fill: var(--fem-heart-icon);
    stroke: var(--fem-heart-icon);
    border-color: var(--fem-heart-icon);
    box-shadow: 0 0 12px rgba(255, 107, 157, 0.7);
}

.horizontal-indicators-container #engine-icon.active {
    fill: var(--fem-accent-pink);
    stroke: var(--fem-accent-pink);
    border-color: var(--fem-accent-pink);
    box-shadow: 0 0 12px rgba(255, 105, 180, 0.7);
}

.horizontal-indicators-container .indicator-icon.active {
    fill: var(--fem-accent-rose);
    stroke: var(--fem-accent-rose);
    border-color: var(--fem-accent-rose);
    background: rgba(255, 182, 193, 0.2);
    box-shadow: 0 0 15px var(--fem-accent-rose), 0 0 30px rgba(255, 182, 193, 0.5);
}

/* Gear Container - Separate and Rounded */
.gear-container-wrapper {
    position: fixed;
    bottom: 175px;
    right: 10px;
    z-index: 22000;
}

/* Health Container - Separate and Rounded */
.health-container-wrapper {
    position: fixed;
    bottom: 115px;
    right: 10px;
    z-index: 22000;
}

/* Gear Stat Bar - Rounded */
.gear-stat-bar {
    width: 80px;
    height: 45px;
    background-color: var(--fem-panel);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: relative;
    border-radius: 15px;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
}

/* Health Stat Bar - Rounded */
.health-stat-bar {
    width: 80px;
    height: 50px;
    background-color: var(--fem-panel);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: relative;
    border-radius: 15px;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
}

/* Gear Label and Value Styles */
.gear-stat-bar .gear-label {
    font-family: var(--font-fem-secondary);
    font-size: 0.7rem;
    text-transform: uppercase;
    margin-bottom: 3px;
    font-weight: 600;
    color: var(--fem-text-secondary);
}

.gear-stat-bar .gear-value {
    font-family: var(--font-fem-primary);
    font-size: 1.2rem;
    font-weight: 700;
    line-height: 1;
    color: var(--fem-accent-pink);
    text-shadow: 0 0 8px rgba(255, 105, 180, 0.5);
}

/* Fuel Label and Value Styles */
.health-stat-bar .health-label {
    font-family: var(--font-fem-secondary);
    font-size: 0.7rem;
    text-transform: uppercase;
    margin-bottom: 3px;
    font-weight: 600;
    color: var(--fem-text-secondary);
}

.health-stat-bar .health-value {
    font-family: var(--font-fem-primary);
    font-size: 0.8rem;
    font-weight: 700;
    line-height: 1;
    color: var(--fem-accent-pink);
    text-shadow: 0 0 8px rgba(255, 105, 180, 0.5);
}

/* Radio Container */
.radio-container {
    position: absolute;
    bottom: 325px;
    right: 10px;
    width: 280px;
    height: 120px;
    background-color: var(--fem-panel);
    border-radius: 12px;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 8px rgba(221, 160, 221, 0.3);
    z-index: 25000;
    display: flex;
    flex-direction: column;
    padding: 8px;
    box-sizing: border-box;
}

.radio-header {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 5px;
}

.radio-title {
    font-family: var(--font-fem-primary);
    font-size: 0.7rem;
    color: var(--fem-accent-pink);
    font-weight: 700;
    letter-spacing: 0.5px;
    margin-bottom: 2px;
}

.radio-station-name {
    font-family: var(--font-fem-secondary);
    font-size: 0.6rem;
    color: var(--fem-text-secondary);
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: center;
    height: 14px;
}

.radio-controls {
    display: flex;
    flex-direction: column;
    gap: 5px;
    flex: 1;
}

.station-navigation {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 15px;
}

.nav-button, .play-pause-button {
    background: rgba(0, 0, 0, 0.1);
    border: 1px solid var(--fem-accent-pink);
    border-radius: 50%;
    color: var(--fem-text-primary);
    font-family: var(--font-fem-primary);
    width: 30px;
    height: 30px;
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    outline: none;
    font-size: 12px;
    transition: all 0.2s ease;
    padding: 0;
}

.nav-button:hover, .play-pause-button:hover {
    background: rgba(0, 0, 0, 0.2);
    box-shadow: 0 0 8px rgba(255, 105, 180, 0.7);
    transform: scale(1.05);
}

.play-pause-button {
    width: 35px;
    height: 35px;
    font-size: 14px;
}

.nav-button:active, .play-pause-button:active {
    transform: scale(0.95);
}

.volume-control {
    display: flex;
    align-items: center;
    gap: 8px;
}

.volume-label {
    font-family: var(--font-fem-secondary);
    font-size: 0.6rem;
    color: var(--fem-text-secondary);
    min-width: 40px;
}

.volume-slider {
    flex: 1;
    height: 4px;
    -webkit-appearance: none;
    background: rgba(0, 0, 0, 0.1);
    border-radius: 2px;
    outline: none;
}

.volume-slider::-webkit-slider-thumb {
    -webkit-appearance: none;
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background: var(--fem-accent-pink);
    cursor: pointer;
    box-shadow: 0 0 3px rgba(255, 105, 180, 0.7);
}

.volume-slider::-moz-range-thumb {
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background: var(--fem-accent-pink);
    cursor: pointer;
    box-shadow: 0 0 3px rgba(255, 105, 180, 0.7);
    border: none;
}

.radio-status {
    font-family: var(--font-fem-secondary);
    font-size: 0.55rem;
    color: var(--fem-accent-pink);
    text-align: center;
    padding: 2px;
    border: 1px solid var(--fem-accent-pink);
    border-radius: 2px;
    height: 16px;
    display: flex;
    align-items: center;
    justify-content: center;
}

/* Radio Info Box */
.radio-info-box {
    position: absolute;
    top: -30px;
    left: 0;
    width: 100%;
    height: 25px;
    background-color: rgba(255, 105, 180, 0.2);
    border: 1px solid var(--fem-accent-pink);
    border-radius: 10px;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 25001;
    animation: pulse 2s infinite;
}

.info-content {
    display: flex;
    align-items: center;
    justify-content: center;
}

.info-text {
    font-family: var(--font-fem-secondary);
    font-size: 0.7rem;
    color: var(--fem-accent-pink);
    font-weight: bold;
    text-shadow: 0 0 5px rgba(255, 105, 180, 0.7);
}

@keyframes pulse {
    0% {
        box-shadow: 0 0 0 0 rgba(255, 105, 180, 0.4);
    }
    70% {
        box-shadow: 0 0 0 5px rgba(255, 105, 180, 0);
    }
    100% {
        box-shadow: 0 0 0 0 rgba(255, 105, 180, 0);
    }
}

/* RPM Gauge Container */
.rpm-gauge-container {
    position: fixed;
    bottom: 225px;
    right: 260px;
    z-index: 21000;
}

/* RPM Gauge - Rounded */
.rpm-gauge {
    position: relative;
    width: 120px;
    height: 50px;
    background-color: var(--fem-panel);
    border-radius: 15px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
}

/* RPM Center Display */
.rpm-center {
    z-index: 5;
    text-align: center;
}

.rpm-display {
    font-family: var(--font-fem-primary);
    font-size: 1.5rem;
    font-weight: 900;
    color: var(--fem-accent-pink);
    line-height: 1;
    margin-bottom: 2px;
    text-shadow: 0 0 8px rgba(255, 105, 180, 0.5);
}

.rpm-unit {
    font-family: var(--font-fem-secondary);
    font-size: 0.6rem;
    color: var(--fem-text-secondary);
}

/* New RPM Bar System - Horizontal and Rounded */
.rpm-bar-new {
    position: fixed;
    right: 10px;
    bottom: 225px;
    width: 240px;
    height: 50px;
    background-color: var(--fem-panel);
    border-radius: 15px;
    z-index: 9999;
    display: flex;
    align-items: center;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
    overflow: hidden;
}

.rpm-bar-fill {
    position: absolute;
    left: 0;
    bottom: 0;
    height: 100%;
    background: linear-gradient(90deg, var(--fem-accent-pink), var(--fem-accent-rose));
    transition: width 0.2s ease;
    width: 0%;
    border-radius: 13px;
}

/* Update RPM bar colors based on level (handled by JS) */
.rpm-bar-fill.warning {
    background: repeating-linear-gradient(
        90deg,
        var(--fem-accent-lavender) 0px,
        var(--fem-accent-lavender) 5px,
        transparent 5px,
        transparent 10px
    );
}

.rpm-bar-fill.danger {
    background: repeating-linear-gradient(
        90deg,
        var(--fem-heart-icon) 0px,
        var(--fem-heart-icon) 5px,
        transparent 5px,
        transparent 10px
    );
}

.rpm-bar-label {
    position: absolute;
    left: 8px;
    top: 50%;
    transform: translateY(-50%);
    color: var(--fem-text-secondary);
    font-family: var(--font-fem-secondary);
    font-size: 0.7rem;
    font-weight: bold;
    z-index: 10;
    pointer-events: none;
}

/* Fuel Gauge Container */
.fuel-gauge-container {
    position: fixed;
    bottom: 60px;
    right: 10px;
    z-index: 21000;
}

/* Fuel Gauge - Rounded */
.fuel-gauge {
    position: relative;
    width: 80px;
    height: 45px;
    background-color: var(--fem-panel);
    border-radius: 15px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    border: 2px solid var(--fem-grid);
    box-shadow: inset 0 0 10px rgba(221, 160, 221, 0.3);
}

/* Fuel Center Display */
.fuel-center {
    z-index: 5;
    text-align: center;
}

.fuel-display {
    font-family: var(--font-fem-primary);
    font-size: 1.2rem;
    font-weight: 900;
    color: var(--fem-accent-pink);
    text-shadow: 0 0 8px rgba(255, 105, 180, 0.5);
    line-height: 1;
}

.fuel-label {
    font-family: var(--font-fem-secondary);
    font-size: 0.4rem;
    color: var(--fem-text-secondary);
    text-transform: uppercase;
    letter-spacing: 1px;
}

/* Health Bar Styles */
.health-bar-background {
    position: absolute;
    bottom: 2px;
    left: 2px;
    right: 2px;
    height: 4px;
    background: rgba(0, 0, 0, 0.1);
    border-radius: 2px;
}

.health-bar-foreground {
    height: 100%;
    width: 100%;
    border-radius: 2px;
    transition: width 0.3s ease, background-color 0.3s ease;
    background-color: var(--fem-accent-pink);
}

/* Update health bar color based on level (handled by JS) */
.health-bar-foreground.warning {
    background-color: var(--fem-accent-lavender);
}

.health-bar-foreground.danger {
    background-color: var(--fem-heart-icon);
}

/* Component Loading States */
.dashboard-container {
    animation: fadeInScale 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards,
               borderGlow 1s ease-in-out 0.8s forwards;
}

.speedometer-container {
    animation: gaugeSlideIn 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.5s forwards;
    opacity: 0;
}

.speed-display {
    animation: textTypewriter 0.4s ease-out 1.0s forwards;
    opacity: 0;
}

.speed-unit {
    animation: textTypewriter 0.4s ease-out 1.2s forwards;
    opacity: 0;
}

.icon:not(.indicator-icon) {
    animation: iconPopIn 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards;
    opacity: 0;
}

.icon:not(.indicator-icon):nth-child(1) { animation-delay: 1.6s; }
.icon:not(.indicator-icon):nth-child(2) { animation-delay: 1.7s; }
.icon:not(.indicator-icon):nth-child(3) { animation-delay: 1.8s; }
.icon:not(.indicator-icon):nth-child(4) { animation-delay: 1.9s; }
.icon:not(.indicator-icon):nth-child(5) { animation-delay: 2.0s; }

/* Indicator icons without entrance animation */
.indicator-icon {
    opacity: 1;
}

.gear-container-wrapper {
    animation: slideInFromBottom 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.4s forwards;
    opacity: 0;
}

.health-container-wrapper {
    animation: slideInFromBottom 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.6s forwards;
    opacity: 0;
}

.stat-bar {
    opacity: 0;
    animation: slideInFromBottom 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards;
}

.stat-bar:nth-child(1) {
    animation-delay: 0.8s;
}

.stat-bar:nth-child(2) {
    animation-delay: 1.0s;
}

.stat-bar:nth-child(3) {
    animation-delay: 1.2s;
}

/* Add loading state for stat values */
.stat-value {
    animation: textTypewriter 0.3s ease-out forwards;
    opacity: 0;
}

.stat-bar:nth-child(1) .stat-value {
    animation-delay: 1.0s;
}

.stat-bar:nth-child(2) .stat-value {
    animation-delay: 1.2s;
}

.stat-bar:nth-child(3) .stat-value {
    animation-delay: 1.4s;
}

.stat-label {
    animation: textTypewriter 0.3s ease-out forwards;
    opacity: 0;
}

.stat-bar:nth-child(1) .stat-label {
    animation-delay: 0.9s;
}

.stat-bar:nth-child(2) .stat-label {
    animation-delay: 1.1s;
}

.stat-bar:nth-child(3) .stat-label {
    animation-delay: 1.3s;
}

/* Gauge Animations */
.rpm-gauge-container {
    animation: gaugeSlideIn 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.6s forwards;
    opacity: 0;
}

.rpm-bar-new {
    animation: rpmBarLoad 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.6s forwards;
    opacity: 0;
}

.fuel-gauge-container {
    animation: gaugeSlideIn 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.8s forwards;
    opacity: 0;
}

.horizontal-indicators-container {
    animation: slideInFromRight 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.8s forwards;
    opacity: 0;
}

.horizontal-indicators-container .icon {
    animation: iconPopIn 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards;
    opacity: 0;
}

.horizontal-indicators-container .icon:nth-child(1) { animation-delay: 1.2s; }
.horizontal-indicators-container .icon:nth-child(2) { animation-delay: 1.3s; }
.horizontal-indicators-container .icon:nth-child(3) { animation-delay: 1.4s; }
.horizontal-indicators-container .icon:nth-child(4) { animation-delay: 1.5s; }
.horizontal-indicators-container .icon:nth-child(5) { animation-delay: 1.6s; }

/* Animations */
@keyframes fadeInScale {
    0% {
        opacity: 0;
        transform: scale(0.3) translateY(50px);
    }
    50% {
        opacity: 0.7;
        transform: scale(1.05);
    }
    100% {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes slideInFromRight {
    0% {
        opacity: 0;
        transform: translateX(100px) scale(0.8);
    }
    100% {
        opacity: 1;
        transform: translateX(0) scale(1);
    }
}

@keyframes slideInFromLeft {
    0% {
        opacity: 0;
        transform: translateX(-100px) scale(0.8);
    }
    100% {
        opacity: 1;
        transform: translateX(0) scale(1);
    }
}

@keyframes slideInFromBottom {
    0% {
        opacity: 0;
        transform: translateY(50px) scale(0.9);
    }
    100% {
        opacity: 1;
        transform: translateY(0) scale(1);
    }
}

@keyframes gaugeSlideIn {
    0% {
        opacity: 0;
        transform: scale(0.3) rotate(10deg);
    }
    100% {
        opacity: 1;
        transform: scale(1) rotate(0deg);
    }
}

@keyframes rpmBarLoad {
    0% {
        opacity: 0;
        transform: translateX(50px) scaleY(0.3);
    }
    100% {
        opacity: 1;
        transform: translateX(0) scaleY(1);
    }
}

@keyframes iconPopIn {
    0% {
        opacity: 0;
        transform: scale(0) rotate(180deg);
    }
    70% {
        transform: scale(1.2) rotate(-10deg);
    }
    100% {
        opacity: 1;
        transform: scale(1) rotate(0deg);
    }
}

@keyframes textTypewriter {
    0% {
        opacity: 0;
        transform: translateY(10px);
    }
    100% {
        opacity: 1;
        transform: translateY(0);
    }
}
