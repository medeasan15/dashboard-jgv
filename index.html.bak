<style>
/* Override: hide old radio panel if present */
.radio-container { display: none !important; }
/* New YouTube Radio panel positioning to match original location */
.yt-radio-container {
  position: absolute; bottom: 325px; right: 10px; width: 320px; min-height: 150px;
  background-color: var(--fem-panel, #ffd6e7); border-radius: 12px; border: 2px solid var(--fem-grid, #e6bcde);
  box-shadow: inset 0 0 8px rgba(221, 160, 221, 0.3); z-index: 26000; display: flex; flex-direction: column; padding: 10px;
  font-family: var(--font-fem-secondary, sans-serif); color: var(--fem-text-secondary, #c71585);
}
.yt-radio-header { display:flex; flex-direction:column; align-items:center; gap:4px; margin-bottom:6px; }
.yt-radio-title { font-family: var(--font-fem-primary, cursive); font-weight:700; font-size: 0.9rem; color: var(--fem-accent-pink, #ff69b4); }
.yt-url-row { display:flex; gap:6px; }
.yt-url-row input { flex:1; padding:6px 8px; border-radius: 8px; border:1px solid var(--fem-grid, #e6bcde); outline: none; }
.yt-url-row button { padding:6px 10px; border-radius: 8px; border:1px solid var(--fem-accent-pink, #ff69b4); background: rgba(0,0,0,0.05); cursor:pointer; }
.yt-controls { display:flex; align-items:center; justify-content:space-between; gap:8px; margin-top:8px; }
.yt-controls .btn { border:1px solid var(--fem-accent-pink, #ff69b4); border-radius: 20px; padding:6px 10px; background: rgba(0,0,0,0.05); cursor:pointer; }
.yt-volume { display:flex; align-items:center; gap:8px; flex:1; }
.yt-volume input[type=range] { flex:1; height: 4px; -webkit-appearance:none; background: rgba(0,0,0,0.1); border-radius:2px; }
